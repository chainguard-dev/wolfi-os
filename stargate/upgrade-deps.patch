From 75b4d9e7d5e8394da2d63377fc12ae2a25e0150f Mon Sep 17 00:00:00 2001
From: Batuhan Apaydin <batuhan.apaydin@chainguard.dev>
Date: Thu, 11 Jan 2024 23:47:40 +0300
Subject: [PATCH] upgrade deps

Signed-off-by: Batuhan Apaydin <batuhan.apaydin@chainguard.dev>
---
 coordinator/graphqlapi/pom.xml | 2 +-
 coordinator/pom.xml            | 6 +++---
 coordinator/testing/pom.xml    | 2 +-
 3 files changed, 5 insertions(+), 5 deletions(-)

diff --git a/coordinator/graphqlapi/pom.xml b/coordinator/graphqlapi/pom.xml
index 6d90fcd0..f3ead98e 100644
--- a/coordinator/graphqlapi/pom.xml
+++ b/coordinator/graphqlapi/pom.xml
@@ -118,7 +118,7 @@
     <dependency>
       <groupId>com.apollographql.federation</groupId>
       <artifactId>federation-graphql-java-support</artifactId>
-      <version>2.0.3</version>
+      <version>2.1.0</version>
     </dependency>
     <!-- Test dependencies -->
     <dependency>
diff --git a/coordinator/pom.xml b/coordinator/pom.xml
index 0cceb45e..b44c5599 100644
--- a/coordinator/pom.xml
+++ b/coordinator/pom.xml
@@ -60,7 +60,7 @@
       Note that this is currently aligned with the metrics version that DropWizard transitively depends on.
       It's probably a good idea to keep the two versions in sync.
     -->
-    <dropwizard.metrics.version>4.2.18</dropwizard.metrics.version>
+    <dropwizard.metrics.version>4.2.23</dropwizard.metrics.version>
     <!--
       Note that this is currently aligned with the Jetty version that DropWizard transitively depends on.
       It's probably a good idea to keep the two versions in sync (or close)
@@ -82,14 +82,14 @@
     <!-- SnakeYAML needs to be synced with jackson-dataformat-yaml
        most of the time, but occasionally higher for CVEs
       -->
-    <snakeyaml.version>1.33</snakeyaml.version>
+    <snakeyaml.version>2.0</snakeyaml.version>
     <swagger-ui.version>3.52.5</swagger-ui.version>
     <swagger-jersey2-jaxrs.version>1.6.3</swagger-jersey2-jaxrs.version>
     <!-- Netty version used by Cassandra 3.11/4.0 (DSE uses different version)
 	 NOTE: upgrading version often breaks persistence backend so make sure
 	 you know what you are doing if/when upgrading
       -->
-    <netty.version>4.1.75.Final</netty.version>
+    <netty.version>4.1.100.Final</netty.version>
     <netty-boringssl.version>2.0.51.Final</netty-boringssl.version>
     <jacoco.version>0.8.8</jacoco.version>
     <junit.version>5.8.2</junit.version>
diff --git a/coordinator/testing/pom.xml b/coordinator/testing/pom.xml
index ed340d9e..6507bb92 100644
--- a/coordinator/testing/pom.xml
+++ b/coordinator/testing/pom.xml
@@ -211,7 +211,7 @@
     <dependency>
       <groupId>com.jayway.jsonpath</groupId>
       <artifactId>json-path</artifactId>
-      <version>2.7.0</version>
+      <version>2.8.0</version>
     </dependency>
     <dependency>
       <groupId>io.projectreactor</groupId>
-- 
